{
  "name": "nuxt-unhead",
  "type": "module",
  "version": "1.4.17",
  "packageManager": "pnpm@8.3.1",
  "license": "MIT",
  "funding": "https://github.com/sponsors/harlan-zw",
  "homepage": "https://github.com/harlan-zw/nuxt-unhead#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harlan-zw/nuxt-unhead.git",
    "directory": "packages/zhead"
  },
  "bugs": {
    "url": "https://github.com/harlan-zw/nuxt-unhead/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint \"**/*.{ts,vue,json,yml}\" --fix",
    "build": "nuxi prepare playground && nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "bumpp package.json --commit --push --tag",
    "test": "vitest ssr"
  },
  "dependencies": {
    "@nuxt/kit": "3.6.5",
    "@unhead/addons": "^1.3.5",
    "fast-glob": "^3.3.1",
    "pathe": "^1.1.1"
  },
  "devDependencies": {
    "playwright-core": "latest",
    "@antfu/eslint-config": "^0.40.2",
    "@nuxt/module-builder": "^0.4.0",
    "@nuxt/test-utils": "3.6.5",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "bumpp": "^9.2.0",
    "cheerio": "1.0.0-rc.12",
    "eslint": "8.47.0",
    "nuxt": "^3.6.5",
    "sass": "^1.66.1",
    "vitest": "^0.34.2"
  }
}

{
  "name": "nuxt-unhead",
  "type": "module",
  "version": "1.0.1",
  "packageManager": "pnpm@7.8.0",
  "license": "MIT",
  "funding": "https://github.com/sponsors/harlan-zw",
  "homepage": "https://github.com/harlan-zw/nuxt-unhead#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harlan-zw/nuxt-unhead.git",
    "directory": "packages/zhead"
  },
  "bugs": {
    "url": "https://github.com/harlan-zw/nuxt-unhead/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "require": "./dist/module.cjs",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint \"**/*.{ts,vue,json,yml}\"",
    "build": "nuxi prepare playground && nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "bumpp package.json --commit --push --tag",
    "test": "vitest ssr"
  },
  "dependencies": {
    "@nuxt/kit": "3.0.0",
    "@unhead/addons": "^1.0.1",
    "@unhead/ssr": "^1.0.1",
    "@vueuse/head": "^1.0.16",
    "fast-glob": "^3.2.12",
    "pathe": "^1.0.0",
    "unplugin-ast": "^0.5.5",
    "zhead": "1.0.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.31.0",
    "@nuxt/kit": "3.0.0",
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/test-utils": "3.0.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "bumpp": "^8.2.1",
    "cheerio": "1.0.0-rc.12",
    "eslint": "8.27.0",
    "nuxt": "^3.0.0",
    "playwright": "^1.28.0",
    "sass": "^1.56.1",
    "vitest": "^0.25.2"
  }
}

{
  "name": "nuxt-unhead",
  "type": "module",
  "version": "1.3.4",
  "packageManager": "pnpm@7.8.0",
  "license": "MIT",
  "funding": "https://github.com/sponsors/harlan-zw",
  "homepage": "https://github.com/harlan-zw/nuxt-unhead#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harlan-zw/nuxt-unhead.git",
    "directory": "packages/zhead"
  },
  "bugs": {
    "url": "https://github.com/harlan-zw/nuxt-unhead/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "require": "./dist/module.cjs",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint \"**/*.{ts,vue,json,yml}\" --fix",
    "build": "nuxi prepare playground && nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "bumpp package.json --commit --push --tag",
    "test": "vitest ssr"
  },
  "dependencies": {
    "@nuxt/kit": "3.1.1",
    "@unhead/addons": "^1.0.20",
    "fast-glob": "^3.2.12",
    "pathe": "^1.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.1",
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/test-utils": "3.1.1",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "bumpp": "^8.2.1",
    "cheerio": "1.0.0-rc.12",
    "eslint": "8.33.0",
    "nuxt": "^3.1.1",
    "playwright": "^1.30.0",
    "sass": "^1.58.0",
    "vitest": "^0.28.3"
  }
}
